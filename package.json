{
  "name": "learn-podman",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "markdownlint \"**/*.md\" --ignore node_modules",
    "lint:fix": "markdownlint \"**/*.md\" --ignore node_modules --fix",
    "spellcheck": "cspell \"**/*.md\" --no-progress",
    "link-check": "find . -name '*.md' -not -path './node_modules/*' -exec markdown-link-check {} \\;",
    "quality": "npm run lint && npm run spellcheck",
    "quality:fix": "npm run lint:fix && npm run spellcheck"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "cspell": "^9.2.2",
    "markdown-link-check": "^3.14.1",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2"
  }
}
